{
  "name": "juno-z",
  "version": "1.0.0",
  "description": "A virtual allowance app for teaching kids about money management.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
