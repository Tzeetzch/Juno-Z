  },
  "homepage": "https://github.com/Tzeetzch/Juno-Z#readme",
  "dependencies": {
    "playwright": "^1.58.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/node": "^25.1.0"
  }
}
