{
  "stryker-config": {
    "project": "JunoBank.Core.csproj",
    "reporters": ["html", "json", "progress", "cleartext"],
    "mutate": [
      "Services/**/*.cs",
      "BackgroundServices/**/*.cs",
      "!Services/I*.cs",
      "!Services/SmtpEmailService.cs",
      "!Services/WeeklySummaryEmailBuilder.cs"
    ],
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },
    "concurrency": 4
  }
}
